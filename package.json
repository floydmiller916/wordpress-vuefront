{
    "name": "vuefront",
    "version": "0.3.67",
    "description": "vuefront",
    "files": [
        "lib",
        "assets",
        "src",
        "seo.js",
        "plugin.js",
        "tailwind"
    ],
    "keywords": [
        "vuefront"
    ],
    "main": "lib/index.js",
    "author": "Dreamvention",
    "license": "MIT",
    "scripts": {
        "build": "shx mkdir -p lib && shx rm -r lib && cross-env NODE_ENV=lib babel src -D --watch --out-dir lib",
        "yalc-watch": "nodemon --delay 6 -e vue,js,scss,json -x yalc push",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "deploy-storybook": "storybook-to-ghpages",
        "lint": "eslint --fix src --ext .vue,.js",
        "watch": "concurrently -k -p \"[{name}]\" -n \"Build,Yalc\" -c \"yellow.bold,cyan.bold\" \"npm run build\" \"npm run yalc-watch\""
    },
    "peerDependencies": {
        "fsevents": "^2.3.2"
    },
    "dependencies": {
        "@jamescoyle/vue-icon": "^0.1.0",
        "@mdi/js": "^5.9.55",
        "@tailwindcss/forms": "^0.3.3",
        "@types/lodash": "^4.14.171",
        "@vuelidate/core": "^2.0.0-alpha.22",
        "@vuelidate/validators": "^2.0.0-alpha.19",
        "axios": "^0.21.1",
        "color": "^3.1.3",
        "concurrently": "^6.2.0",
        "core-js": "^3.15.2",
        "form-data": "^4.0.0",
        "global": "^4.4.0",
        "graphql": "^15.5.1",
        "graphql-tag": "^2.12.5",
        "lodash": "^4.17.21",
        "lodash-es": "^4.17.21",
        "mini-svg-data-uri": "^1.3.3",
        "omit-deep-lodash": "^1.1.5",
        "tailwindcss": "2.2.4",
        "traverse": "^0.6.6",
        "vue": "^3.1.5",
        "vue-image-lightbox": "^7.2.0",
        "vue-lazy-hydration": "^2.0.0-beta.4",
        "vue-skeletor": "^1.0.6"
    },
    "devDependencies": {
        "@babel/cli": "^7.14.5",
        "@babel/core": "^7.14.6",
        "@babel/preset-env": "^7.14.7",
        "@nuxtjs/eslint-config": "^6.0.1",
        "@nuxtjs/eslint-config-typescript": "^6.0.1",
        "cross-env": "^7.0.3",
        "eslint": "^7.29.0",
        "nodemon": "^2.0.9",
        "prettier": "^2.3.2",
        "shx": "^0.3.3"
    }
}
