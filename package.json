{
    "name": "vuefront",
    "version": "0.4.6",
    "description": "vuefront",
    "files": [
        "lib",
        "es5",
        "assets",
        "src",
        "seo.js",
        "plugin.js",
        "tailwind"
    ],
    "keywords": [
        "vuefront"
    ],
    "main": "lib/index.js",
    "author": "Dreamvention",
    "license": "MIT",
    "scripts": {
        "build-es": "shx mkdir -p es5 && shx rm -r es5 && cross-env NODE_ENV=es5 babel src -D --watch --out-dir es5",
        "build": "shx mkdir -p lib && shx rm -r lib && cross-env NODE_ENV=lib babel src -D --watch --out-dir lib",
        "yalc-watch": "nodemon --delay 6 -e vue,js,scss,json -x yalc push",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "deploy-storybook": "storybook-to-ghpages",
        "lint": "eslint --fix src --ext .vue,.js",
        "watch": "concurrently -k -p \"[{name}]\" -n \"Build,Yalc\" -c \"yellow.bold,cyan.bold\" \"npm run build\" \"npm run yalc-watch\""
    },
    "peerDependencies": {
        "fsevents": "^2.3.2"
    },
    "dependencies": {
        "@jamescoyle/vue-icon": "^0.1.2",
        "@mdi/js": "^5.9.55",
        "@tailwindcss/forms": "^0.3.3",
        "@types/lodash": "^4.14.172",
        "@vuelidate/core": "^2.0.0-alpha.24",
        "@vuelidate/validators": "^2.0.0-alpha.21",
        "axios": "^0.21.1",
        "color": "^4.0.1",
        "concurrently": "^6.2.1",
        "form-data": "^4.0.0",
        "global": "^4.4.0",
        "graphql": "^15.5.2",
        "graphql-tag": "^2.12.5",
        "lodash": "^4.17.21",
        "lodash-es": "^4.17.21",
        "mini-svg-data-uri": "^1.3.3",
        "omit-deep-lodash": "^1.1.5",
        "tailwindcss": "2.2.9",
        "traverse": "^0.6.6",
        "vue-easy-lightbox": "^1.2.3",
        "vue-image-lightbox": "^7.2.0",
        "vue-lazy-hydration": "^2.0.0-beta.4",
        "vue-skeletor": "^1.0.6"
    },
    "devDependencies": {
        "@babel/cli": "^7.14.8",
        "@babel/core": "^7.15.0",
        "@babel/preset-env": "^7.15.0",
        "@types/lodash-es": "^4.17.4",
        "@typescript-eslint/eslint-plugin": "^4.30.0",
        "@typescript-eslint/parser": "^4.30.0",
        "cross-env": "^7.0.3",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-vue": "^7.17.0",
        "nodemon": "^2.0.12",
        "prettier": "^2.3.2",
        "sass-loader": "^12.1.0",
        "shx": "^0.3.3",
        "storybook-addon-vue-info": "^1.4.2",
        "typescript": "^4.4.2",
        "vite": "^2.5.2",
        "vue": "3.2.6",
        "vue-i18n": "^8.25.0",
        "vue-loader": "^15.9.8",
        "vue-router": "^3.5.2",
        "vue-template-compiler": "^2.6.14"
    }
}
